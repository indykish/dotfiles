      "ansible-playbook *": "allow",
      "helm *": "allow",
      "docker *": "allow",
      "kubectl *": "allow",
      "ssh *": "allow",
      "curl *": "allow",
      "pyenv *": "allow",
      "uv *": "allow",
      "python3 *": "allow",
