      "awk *": "allow",
      "gawk *": "allow",
      "sed *": "allow",
      "gsed *": "allow",
      "diff *": "allow",
      "cmp *": "allow",
      "open *": "allow",
      "pbcopy *": "allow",
      "pbpaste *": "allow",
      "mdls *": "allow",
      "defaults *": "allow",
      "launchctl *": "allow",
      "dtruss *": "allow",
      "lsof *": "allow",
      "vm_stat *": "allow",
